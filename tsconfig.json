{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "target": "es6",
    "sourceMap": false,
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": false,
    "noUnusedLocals": true,
    "removeComments": true,
    "preserveConstEnums": true
  }
}
