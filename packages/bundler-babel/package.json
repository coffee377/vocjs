{
  "name": "@vocjs/bundler-babel",
  "version": "0.0.1",
  "description": "babel 打包工具",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "typing/index.d.ts",
  "bin": {
    "bundler-babel": "./bin/babel.js"
  },
  "scripts": {
    "prepack": "rimraf esm lib typing",
