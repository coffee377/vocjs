{
  "name": "dts-helper",
  "version": "1.1.0",
  "description": "TypeScript 声明文件生成工具",
  "bin": {
    "dts": "./bin/cli.js"
  },
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "bin",
    "docs",
    "lib",
    "types",
    "README.md"
  ],
  "scripts": {
    "dts": "node bin/cli.js -d types",
    "compile": "tsc",
    "build": "babel src -d lib -x .ts",
    "build:watch": "babel src -d lib -x .ts -w",
    "test": "jest"
  },
  "keywords": [
    "TypeScript",
    "tsc",
    "dts",
    "declaration",
    "tool"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^25.1.4",
    "commander": "^5.0.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.149",
    "babel-jest": "^25.1.0",
    "jest": "^25.1.0",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  }
}
