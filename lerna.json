{
  "version": "independent",
  "packages": ["packages/*"],
  "useWorkspaces": true,
  "npmClient": "yarn",
  "command": {
    "bootstrap": {
      "npmClientArgs": []
    },
    "publish": {
      "allowBranch": ["master", "dev", "feature/*"],
      "ignoreChanges": ["**/*.md", "**/*.test.ts", "**/*.e2e.ts", "**/fixtures/**", "**/test/**"]
    }
  }
}
